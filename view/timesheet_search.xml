<?xml version="1.0"?>
<form>
    <label name="employee"/>
    <field name="employee"/>
    <label name="date"/>
    <field name="date"/>

    <group col="6" id="search_filters">
        <label name="user_filter"/>
        <field name="user_filter"/>
        <label name="month_filter"/>
        <field name="month_filter"/>
        <label name="year_filter"/>
        <field name="year_filter"/>
    </group>

    <group col="2" id="buttons">
        <button name="button_search" string="Search" icon="tryton-search"/>
        <button name="button_clear" string="Clear" icon="tryton-clear"/>
    </group>

    <group col="4" id="timesheet_search_filters">
        <label name="company"/>
        <field name="company"/>
        <label name="work"/>
        <field name="work"/>
    </group>
</form>
